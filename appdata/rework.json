    "::order_rework": {
        "param":"param",
        "fn_key":{"order_id":"eid"},
        "data_params": {"rw_order_id":"title_data.rw_order_id", "rw_container_id":"title_data.rw_container_id"},
        "data_validate": {"rw_unit_sn":"validate_data"},
        "title_template":{
            "data_source":"title_data",
            "fields":[
                {"f_id":"title", "d_name":"Order", "f_element":"h3"},
                {"f_id":"model", "d_name":"Model", "f_element":"h3"},
                {"f_id":"container", "d_name":"Container", "f_element":"h3"},
                {"f_id":"container_sq", "d_name":"Container No.", "f_element":"h3"}
            ]
        },
        "action_template":{
            "fields":[
                {"f_id":"rw_unit_sn", "f_event_id":"sn_insert", "f_data_endpoint":"/api/rework/rw_sn_insert", "f_data_validate":"checkInputValidatePattern", "d_name":"Scan Serial No.", "f_type":"text", "f_element":"input", "f_focus":true}
            ]
        } ,
        "table_template":{
            "data_source":"table_data",
            "fields":[
                {"f_id":"eid", "d_name":"ID", "f_element":"thead", "f_sort":"^"},
                {"f_id":"rw_unit_sn", "d_name":"Serial No.", "f_element":"thead", "f_sort":"^"},
                {"f_id":"create_date", "d_name":"Create Date", "f_element":"thead", "f_sort":"^"}
            ],
            "events":[]
        },
        "summ_template":{
            "summ_source":"summ_data",
            "fields":[
                {"f_id":"quantity", "d_name":"SN Count", "f_element":"label"}
            ]
        },
        "data_endpoint": {
            "data": {
                "handler": "_query_queue",
                "query": [
                    {"rw_container_id":"INSERT INTO rw_containers (title, rw_order_id, container_sq) SELECT ro.title || '-' || (MAX(COALESCE(rc.container_sq, 0))+1) as c_title, ro.eid, (MAX(COALESCE(rc.container_sq, 0))+1) as seq FROM rw_orders ro LEFT OUTER JOIN rw_containers rc WHERE ro.eid = %(eid)s;"},
                    {"validate_data":"SELECT rst.sn_startswith, rst.sn_contains, rst.sn_length FROM rw_sn_type rst INNER JOIN rw_orders ro ON rst.eid = ro.rw_sn_type_id WHERE ro.eid = %(eid)s;"},
                    {"title_data":"SELECT ro.eid as rw_order_id, ro.title, rm.title as model, rc.eid as rw_container_id, rc.title as container, rc.container_sq as seq FROM rw_orders ro INNER JOIN rw_containers rc ON ro.eid = rc.rw_order_id LEFT JOIN rw_model rm ON ro.rw_model_id = rm.eid WHERE rc.eid = %(rw_container_id)s;"},
                    {"table_data":"SELECT * FROM rw_unit_serials rus WHERE rus.rw_container_id = %(rw_container_id)s;"},
                    {"summ_data":"SELECT count(*) as quantity FROM rw_unit_serials rus WHERE rus.rw_container_id = %(rw_container_id)s;"}
                ]
            }
        },
        "auth": ["admin"]
    },
