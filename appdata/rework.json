                {"f_id":"status", "d_name":"Status", "f_type":"thead", "f_sort":"^"},
                {"f_id":"remark", "d_name":"Remark", "f_type":"thead", "f_sort":"^"},
                {"f_id":"create_date", "d_name":"Create Date", "f_type":"thead"}
            ],
            "events":[
                {"f_id":"eid", "d_name":"View", "f_type":"button", "f_event_id":"view", "handler": "/js/part/fn_modal_view.js", "meta_endpoint": "/api/meta/route_meta", "meta_section": "rework.json::order_view"},
                {"f_id":"eid", "d_name":"Detail", "f_type":"button", "f_event_id":"detail", "handler": "/js/part/fn_table_rework.js", "container":"self", "meta_endpoint": "/api/meta/route_meta", "meta_section": "rework.json::order_detail"}
            ]
        },
        "data_endpoint": {
            "data": {
                "handler": "read",
                "query": "SELECT ro.*, ac.acct_name, rm.title as model_name, rcq.quantity FROM rw_orders ro LEFT OUTER JOIN accounts ac ON ro.vendor_id = ac.eid LEFT OUTER JOIN rw_model rm ON ro.rw_model_id = rm.eid LEFT OUTER JOIN (SELECT rc.rw_order_id, count(*) as quantity FROM rw_containers rc GROUP BY rc.rw_order_id) as rcq on ro.eid = rcq.rw_order_id ORDER BY ro.eid DESC"
            }
        },
        "auth": ["admin"]
    },
