    "::order_detail": {
        "title": "order View Meta",
        "param":"param",
        "fn_key":{"order_id":"eid"},
        "title_template":{"f_id":"title","f_element":"label","f_data":"/api/rework/order_label","f_display_fields":{"title":"Order Title","status":"Status","create_date":"Creation Date"}},
        "action_bar": [
            {"f_id":"eid", "f_event_id":"report", "d_name":"Report", "f_type":"button", "handler":"self", "container":"self", "meta_endpoint": "/api/meta/route_meta", "meta_section": "rework.json::order_report"},
            {"f_id":"eid", "f_event_id":"new_container", "d_name":"New Pallet", "f_type":"button", "handler":{"rework":"/js/part/fn_v2_modal_rework.js", "repair":"/js/part/fn_modal_repair.js"}, "container":"dialog", "meta_endpoint": "/api/meta/route_meta", "meta_section":{"rework":"rework.json::order_rework","repair":"rework.json::order_repair"}}
        ],
        "table_template":{
            "table_key":"eid",
            "fields":[
                {"f_id":"eid", "d_name":"ID", "f_type":"thead", "f_sort":"^"},
                {"f_id":"title", "d_name":"Pallet Label", "f_type":"thead", "f_sort":"^"},
                {"f_id":"container_sq", "d_name":"Container No.", "f_type":"thead", "f_sort":"^"},
                {"f_id":"quantity", "d_name":"Quantity", "f_type":"thead"},
                {"f_id":"remark", "d_name":"Remark", "f_type":"thead", "f_sort":"^"},
                {"f_id":"create_date", "d_name":"Create Date", "f_type":"thead", "f_sort":"^"}
            ],
            "events":[]
        },
        "data_endpoint": {
            "data": {
                "handler": "read",
                "query": "SELECT rc.*, ro.order_type, ruc.quantity FROM rw_containers rc INNER JOIN rw_orders ro ON rc.rw_order_id = ro.eid LEFT OUTER JOIN (SELECT rus.rw_container_id, count(*) as quantity FROM rw_unit_serials rus GROUP BY rus.rw_container_id) as ruc ON rc.eid = ruc.rw_container_id WHERE rc.rw_order_id = %(eid)s ORDER BY eid DESC;"
            }
        },
        "auth": ["admin"]
    },
