    "::ship_packing_list": {
        "data_endpoint": {
            "data": {
                "handler": "generate_pdf",
                "query": "SELECT o.title as order_title, i.category, i.v_sku, i.v_name, i.v_description, ol.quantity, s.pallet_count, a.acct_name as vendor_title, t.acct_name as trucker_title FROM order_lines ol INNER JOIN orders o ON ol.order_id = o.eid INNER JOIN items i ON ol.item_id = i.eid INNER JOIN accounts a ON o.vendor_id = a.eid LEFT OUTER JOIN shipments s ON o.link_order = s.work_id LEFT OUTER JOIN accounts t ON s.trucker_id = t.eid AND t.acct_type = 'truck' WHERE ol.order_id = :eid",
                "file_handle": "packingListA4",
                "content_type": "application/pdf",
                "file_name": "ship_packing_list.pdf"
            },
            "data_type":"json"
        },
        "auth": ["admin","manager"]
    },
